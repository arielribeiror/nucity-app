{
  "colors": {
    "primaryColor": "#4059AD",
    "secondaryColor": "#6B9AC4",
    "tertiaryColor": "#97D8C4",
    "dark": "#363537",
    "light": "#EFF2F1",
    "info": "#17A398",
    "success": "#0CCE6B",
    "warning": "#F4B942",
    "danger": "#FF3C38",
    "grayColor01": "#5E5D5F",
    "grayColor02": "#D6D6D7"
  },
  "expo": {
    "name": "nucity",
    "slug": "nucity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo nucity deseja ter acesso a galeria do seu dispositivo."
        }
      ]
    ]
  }
}
